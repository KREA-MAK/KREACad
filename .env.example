# KreaCAD React Environment Configuration

# API Configuration
VITE_API_URL=http://localhost:3000
VITE_API_TIMEOUT=30000

# Application
VITE_ENVIRONMENT=development
VITE_VERSION=0.18.0
VITE_ENABLE_ANALYTICS=false

# External Libraries
VITE_DRACO_DECODER_URL=/libs/draco_decoder.wasm
VITE_RHINO3DM_URL=/libs/rhino3dm.wasm

# Feature Flags
VITE_ENABLE_MEASUREMENTS=true
VITE_ENABLE_SNAPSHOTS=true
VITE_ENABLE_SHARING=true
VITE_ENABLE_EXPORT=true

# Logging
VITE_LOG_LEVEL=info
